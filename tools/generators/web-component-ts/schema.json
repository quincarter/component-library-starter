{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "web-component-ts",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Component Name (my-component)",
      "x-prompt": "Component Name (my-component)",
      "pattern": "^[a-zA-Z]+-",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": ["name"]
}
