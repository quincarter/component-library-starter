{
  "name": "quincarter",
  "description": "Webcomponent quincarter following open-wc recommendations",
  "license": "MIT",
  "author": "quincarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint:fix",
    "format": "nx run-many --target=format",
    "test": "nx run-many --target=test",
    "docs:md": "nx run-many --target=docs:md",
    "build": "npm-run-all --sequential rimraf-dist && nx run-many --target=build",
    "tsc:watch": "nx run-many --target=tsc:watch",
    "changeset:add": "changeset && changeset version",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "npm run analyze -- --exclude dist && build-storybook",
    "new-component:ts": "nx workspace-generator web-component-ts $1",
    "postnew-component:ts": "yarn install && yarn lint:fix && yarn format && yarn build && yarn test && yarn docs:md",
    "rimraf-dist": "rimraf packages/**/dist",
    "rimraf": "rimraf --",
    "pretty-quick": "pretty-quick --staged --bail",
    "analyze": "cem analyze --litelement",
    "start": "npm-run-all --parallel tsc:watch storybook",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.4",
    "lit": "^2.3.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^14.2.6",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.16.4",
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@nrwl/devkit": "^15.0.2",
    "@nrwl/eslint-plugin-nx": "^15.0.2",
    "@nrwl/jest": "^15.0.2",
    "@nrwl/linter": "^15.0.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "^15.0.2",
    "@open-wc/building-rollup": "^2.0.2",
    "@open-wc/eslint-config": "^8.0.2",
    "@open-wc/testing": "next",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-cssresources": "^6.2.9",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/web-components": "^6.5.13",
    "@types/jest": "27.4.1",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-storybook": "next",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "@web/test-runner": "^0.15.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "css-loader": "^6.7.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.6",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "nx": "^15.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "storybook-readme": "^5.0.9",
    "style-loader": "^3.3.1",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4",
    "web-component-analyzer": "^2.0.0-next.4"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc",
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(tools/generators/web-component-ts/**/*)*.json": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "customElements": "custom-elements.json",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "prod",
          "^prod"
        ],
        "dependsOn": [
          {
            "projects": "dependencies",
            "target": "build"
          },
          {
            "projects": "dependencies",
            "target": "test"
          }
        ]
      },
      "test": {
        "dependsOn": [
          {
            "projects": "dependencies",
            "target": "build"
          }
        ]
      }
    }
  }
}
