{
  "name": "@quincarter/test-ts-component",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "module": "dist/@quincarter/test-ts-component.esm.js",
  "scripts": {
    "build": "tsc && rollup -c rollup.config.js",
    "test": "tsc && web-test-runner --coverage",
    "tsc:watch": "tsc --watch",
    "lint": "eslint --ext .ts,.html --ignore-path .eslintignore && prettier \"**/*.ts\" --check --ignore-path .eslintignore",
    "format": "eslint --ext .js,.html --fix --ignore-path .eslintignore && prettier \"**/*.ts\" --write --ignore-path .eslintignore"
  },
  "peerDependencies": {
    "@open-wc/eslint-config": "*",
    "@open-wc/testing": "*",
    "@web/test-runner": "*",
    "@rollup/plugin-babel": "*",
    "@rollup/plugin-node-resolve": "*",
    "@web/rollup-plugin-import-meta-assets": "*",
    "eslint": "*",
    "lit": "2.x",
    "prettier": "*",
    "rollup": "*",
    "rollup-plugin-terser": "*",
    "typescript": "*"
  }
}
