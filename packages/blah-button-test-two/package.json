{
  "name": "@quincarter/blah-button-test-two",
  "version": "1.5.0",
  "license": "MIT",
  "exports": {
    ".": "./out-tsc/index.js",
    "./Blah": {
      "import": "./out-tsc/src/blah-button-test-two.js",
      "require": "./out-tsc/src/blah-button-test-two.js",
      "types": "./out-tsc/src/blah-button-test-two.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./out-tsc/index.d.ts"
      ],
      "Blah": [
        "./out-tsc/src/blah-button-test-two.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "out-tsc"
  ],
  "private": false,
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "yarn run docs:md",
    "docs:md": "wca analyze ./src/*.ts --format markdown --outFile README.md",
    "test": "tsc && web-test-runner --coverage",
    "tsc:watch": "tsc --watch",
    "lint": "eslint --ext .ts,.html --ignore-path .eslintignore && prettier \"**/*.ts\" --check --ignore-path .eslintignore",
    "lint:fix": "eslint --ext .ts,.html --ignore-path .eslintignore && prettier \"**/*.ts\" --fix --ignore-path .eslintignore",
    "format": "eslint --ext .js,.html --fix --ignore-path .eslintignore && prettier \"**/*.ts\" --write --ignore-path .eslintignore"
  },
  "dependencies": {
    "lit": "2.x"
  }
}
