{
  "name": "@quincarter/some-button",
  "version": "1.1.2",
  "license": "MIT",
  "main": "dist/@quincarter/some-button.js",
  "module": "dist/@quincarter/some-button.js",
  "exports": {
    ".": "./out-tsc/index.js",
    "./SomeButton": {
      "import": "./out-tsc/src/some-button.js",
      "require": "./out-tsc/src/some-button.js",
      "types": "./out-tsc/src/some-button.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./out-tsc/index.d.ts"
      ],
      "SomeButton": [
        "./out-tsc/src/some-button.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "out-tsc"
  ],
  "private": false,
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "yarn run docs:md",
    "docs:md": "wca analyze ./src/*.ts --format markdown --outFile README.md",
    "test": "tsc && web-test-runner --coverage",
    "tsc:watch": "tsc --watch",
    "lint": "eslint --ext .ts,.html --ignore-path .eslintignore && prettier \"**/*.ts\" --check --ignore-path .eslintignore",
    "lint:fix": "eslint --ext .ts,.html --ignore-path .eslintignore && prettier \"**/*.ts\" --fix --ignore-path .eslintignore",
    "format": "eslint --ext .js,.html --fix --ignore-path .eslintignore && prettier \"**/*.ts\" --write --ignore-path .eslintignore"
  },
  "dependencies": {
    "lit": "2.x"
  }
}
