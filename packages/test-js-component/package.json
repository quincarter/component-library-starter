{
  "name": "@quincarter/test-js-component",
  "version": "1.2.2",
  "license": "MIT",
  "main": "dist/@quincarter/test-js-component.js",
  "module": "dist/@quincarter/test-js-component.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "private": false,
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "docs:md": "wca analyze src/test-js-component.js --format markdown --outFile README.md",
    "test": "wtr --coverage",
    "lint": "eslint --ext .js,.html --ignore-path .eslintignore && prettier \"**/*.js\" --check --ignore-path .eslintignore",
    "lint:fix": "eslint --ext .js,.html --ignore-path .eslintignore && prettier \"**/*.js\" --fix --ignore-path .eslintignore",
    "format": "eslint --ext .js,.html --fix --ignore-path .eslintignore && prettier \"**/*.js\" --write --ignore-path .eslintignore"
  },
  "peerDependencies": {
    "lit": "2.x"
  }
}
